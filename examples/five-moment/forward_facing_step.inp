set Application = FiveMoment
set n_dims = 2
set t_end = 3.000
set fields_enabled = false

set fe_degree = 4

set n_boundaries = 6
subsection geometry
    set GridType = ForwardFacingStep
    set RefinementFactor = 2
end

set gas_gamma = 1.4

set n_writeout_frames = 100

subsection Species_1
    subsection InitialCondition
        set VariablesType = Primitive
        set Function expression = 1.4; 3.0; 0.0; 1.0
    end

    subsection BoundaryConditions
        set 0 = Inflow
        set 1 = Wall
        set 2 = Wall
        set 3 = Wall
        set 4 = Outflow
        set 5 = Wall

        subsection 0_Inflow
            set VariablesType = Primitive
            set Function expression = 1.4; 3.0; 0.0; 1.0
        end
    end
end
